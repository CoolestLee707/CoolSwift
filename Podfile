pod 'SwiftyJSON'        # 处理JSON数据
pod 'Alamofire'         # 网络请求
pod 'SnapKit'           # 约束布局
pod 'CLImagePickerTool'
pod 'Kingfisher'        # 图片加载
pod 'IQKeyboardManagerSwift'    # 键盘扩展
end
