platform :ios, '9.0'
target 'MySwift' do
source ‘https://github.com/CocoaPods/Specs.git’

use_frameworks!

pod 'SwiftyJSON'    #JSON解析类
pod 'MJRefresh'     # 刷新数据
pod 'FDFullscreenPopGesture'    #全屏右滑返回
